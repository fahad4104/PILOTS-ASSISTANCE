generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Manual {
  id           String   @id @default(cuid())
  key          String   @unique
  originalName String
  blobUrl      String
  blobPathname String
  openaiFileId String
  createdAt    DateTime @default(now())
}

model Flight {
  id             String   @id @default(cuid())
  user_id        String
  date           String
  flight_number  String
  departure      String
  destination    String
  departure_time String
  arrival_time   String
  aircraft       String
  status         String   @default("scheduled")
  createdAt      DateTime @default(now())

  @@map("flights")
}




