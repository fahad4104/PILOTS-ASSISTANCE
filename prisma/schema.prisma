generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Manual {
  id           String   @id @default(cuid())
  key          String   @unique
  originalName String
  blobUrl      String
  blobPathname String
  openaiFileId String
  createdAt    DateTime @default(now())
}



